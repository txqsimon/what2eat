.btn-del{
  position: fixed;
  z-index: 200;
  width: 100%;
  height: 34px;
  bottom: 0;
  padding-top: 5px;
  border-top: 1px solid #eee;
}
.btn-del button{
  float: right;
  right: 20rpx;
  margin: 0 5rpx;
}


.create-box{
  height: 100%;
  justify-content: space-between;  
  padding: 50rpx 100rpx; 
  box-sizing: border-box;
}
.label-text{
  position: relative;
  display: inline-block;
  padding: 10rpx 30rpx;
  text-align: center;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  margin: 10rpx;
  line-height: 1rem;
  vertical-align: middle;
}

.icon-select{
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  size: 1rem;
}

.text{
  color: #333;
  font-size: 13px;
  font-family: Microsoft Yahei;
}

.label-add{
  display: inline-block;
  margin: 10rpx;
  padding: 5rpx 10rpx;
  border: 1rpx solid transparent;
  vertical-align: middle; 
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:13px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-add:before { 
  content: "\e634";
  font-size: 22px;
  font-weight: bold;
  color: #ccc;
}

.add-mask{
  position: fixed;
  z-index: 100;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  background-color:rgba(15, 15, 26, 0.3);  
}
.textarea-view{
  background:#fff;
  min-height:150px;
  width:100%;
  top: -1000px;
  position: absolute;
  z-index: 200;
}
.textarea{
  border: 1px solid #eee;
  border-radius: 4px;
  height: 100px;
  width: 90%;
  margin: 0 auto;
  padding: 15rpx;
  font-size: 13px;
}
.textarea-bottom{
  width: 94%;
  margin: 0 auto;
  padding: auto 0;
  margin-top:5rpx;
  /* line-height: 2; */
}
.textarea-bottom .label{
  font-size: 12px;
  color: #999;
  /* margin-left: 2.5%; */
}
.textarea-bottom button{
  float: right;
  line-height: 2;
  margin:5rpx 0;
}



@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}



